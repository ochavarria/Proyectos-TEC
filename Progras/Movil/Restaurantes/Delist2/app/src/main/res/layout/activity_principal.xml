<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="delist2.reque.com.delist2.Principal">
    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Registrar Sitio"
        android:id="@+id/button_registrar"
        android:layout_marginTop="40dp"
        android:background="#6aca25"
        android:layout_alignParentTop="true"
         />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Buscar Sitio"
        android:id="@+id/button_buscar"
        android:background="#6aca25"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/button_registrar"
        />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Administrar Listas Compartidas"
        android:id="@+id/button_LC"
        android:background="#6aca25"
        android:layout_centerVertical="true"
        />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Administrar Listas Privadas"
        android:id="@+id/button_lp"
        android:layout_marginTop="48dp"
        android:background="#6aca25"
        android:layout_below="@+id/button_LC"
         />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Agregar_Amigos"
        android:id="@+id/button_agregar"
        android:background="#6aca25"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/button_lp"
        />

</RelativeLayout>
